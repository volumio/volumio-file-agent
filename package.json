{
  "name": "volumio-file-agent",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "build": "rm -rf dist && npm run openApi:json && tsc --build tsconfig.json",
    "start": "node dist/main",
    "start:ts": "DEBUG=fa:database:sqlite,fa:database:sqlite:usecase,fa:httpServer node -r ts-node/register src/main.ts",
    "openApi:doc": "spot docs src/HTTPServer/OpenApiBackend/contract.ts",
    "openApi:json": "spot generate -c src/HTTPServer/OpenApiBackend/contract.ts -g openapi3 -l json -o ./src/HTTPServer/OpenApiBackend",
    "try": "npm run build && npm run start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/volumio/volumio-file-agent.git"
  },
  "dependencies": {
    "async": "^3.2.0",
    "better-sqlite3": "^6.0.1",
    "body-parser": "^1.19.0",
    "core-js": "^3.6.5",
    "debug": "^4.1.1",
    "eventemitter3": "^4.0.0",
    "express": "^4.17.3",
    "fp-ts": "^2.5.3",
    "heapdump": "^0.3.15",
    "lodash": "^4.17.15",
    "module-alias": "^2.2.2",
    "music-metadata": "^6.3.7",
    "on-finished": "^2.3.0",
    "openapi-backend": "^3.5.1",
    "performance-now": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.5",
    "shortid": "^2.2.15",
    "sqlite3": "^4.1.1",
    "typeorm": "^0.2.24",
    "zeromq": "^5.2.0"
  },
  "devDependencies": {
    "@airtasker/spot": "1.0.1",
    "@types/async": "^3.2.0",
    "@types/better-sqlite3": "^5.4.0",
    "@types/core-js": "^2.5.3",
    "@types/express": "^4.17.6",
    "@types/heapdump": "^0.3.1",
    "@types/lodash": "^4.14.149",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^13.9.5",
    "@types/on-finished": "^2.3.1",
    "@types/shortid": "0.0.29",
    "@types/zeromq": "^4.6.3",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-simple-import-sort": "^5.0.2",
